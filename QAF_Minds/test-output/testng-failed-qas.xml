<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="QAF Demo">
  <test thread-count="5" name="BDD Test">
    <method-selectors>
      <method-selector>
        <script language="beanshell">
          <![CDATA[testngMethod.getMethodName().equalsIgnoreCase("Add Mind")||testngMethod.getMethodName().equalsIgnoreCase("Get all employee data")||testngMethod.getMethodName().equalsIgnoreCase("Get Mind")]]>
        </script>
      </method-selector>
    </method-selectors>
    <classes>
      <class name="com.qmetry.qaf.automation.step.client.text.BDDTestFactory"/>
    </classes>
  </test> <!-- BDD Test -->
</suite> <!-- QAF Demo -->
